<?xml version="1.0" encoding="utf-8"?>
<android.support.design.widget.CoordinatorLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <!-- MapView-->
    <com.esri.android.map.MapView
        android:id="@+id/map"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        mapoptions.MapType =   "Topo"
        mapoptions.ZoomLevel = "18"
        mapoptions.center = "34.1822, -117.3235">
    </com.esri.android.map.MapView>
    <!-- android:layout_below="@+id/directionsSpinner" -->


    <android.support.design.widget.FloatingActionButton
        android:layout_width = "50dp"
        android:layout_height = "50dp"
        android:layout_gravity = "bottom|end"
        android:layout_marginRight = "@dimen/fab_margin_right"
        android:layout_marginBottom = "330dp"
        android:id="@+id/fab_five"
        android:src="@drawable/five"
        android:elevation="6dp"
        app:pressedTranslationZ="12dp"
        app:backgroundTint="@color/white"
        android:visibility="invisible"
        >
    </android.support.design.widget.FloatingActionButton>

    <android.support.design.widget.FloatingActionButton
        android:layout_width = "50dp"
        android:layout_height = "50dp"
        android:layout_gravity = "bottom|end"
        android:layout_marginRight = "@dimen/fab_margin_right"
        android:layout_marginBottom = "280dp"
        android:id="@+id/fab_fourth"
        android:src="@drawable/four"
        android:elevation="6dp"
        app:pressedTranslationZ="12dp"
        app:backgroundTint="@color/white"
        android:visibility="invisible"
        >
    </android.support.design.widget.FloatingActionButton>

    <android.support.design.widget.FloatingActionButton
        android:layout_width = "50dp"
        android:layout_height = "50dp"
        android:layout_gravity = "bottom|end"
        android:layout_marginRight = "@dimen/fab_margin_right"
        android:layout_marginBottom = "230dp"
        android:id="@+id/fab_third"
        android:src="@drawable/three"
        android:elevation="6dp"
        app:pressedTranslationZ="12dp"
        app:backgroundTint="@color/white"
        android:visibility="invisible"
        >
    </android.support.design.widget.FloatingActionButton>

    <android.support.design.widget.FloatingActionButton
        android:layout_width = "50dp"
        android:layout_height = "50dp"
        android:layout_gravity = "bottom|end"
        android:layout_marginRight = "@dimen/fab_margin_right"
        android:layout_marginBottom = "180dp"
        android:id="@+id/fab_second"
        android:src="@drawable/two"
        android:elevation="6dp"
        app:pressedTranslationZ="12dp"
        app:backgroundTint="@color/white"
        android:visibility="invisible"
        >

    </android.support.design.widget.FloatingActionButton>

    <android.support.design.widget.FloatingActionButton
        android:layout_width = "50dp"
        android:layout_height = "50dp"
        android:layout_gravity = "bottom|end"
        android:layout_marginRight = "@dimen/fab_margin_right"
        android:layout_marginBottom = "130dp"
        android:id="@+id/fab_first"
        android:src="@drawable/one"
        android:elevation="6dp"
        app:pressedTranslationZ="12dp"
        app:backgroundTint="@color/white"
        android:visibility="invisible"
        >
    </android.support.design.widget.FloatingActionButton>

    <android.support.design.widget.FloatingActionButton
        android:layout_width = "50dp"
        android:layout_height = "50dp"
        android:layout_gravity = "bottom|end"
        android:layout_marginRight = "@dimen/fab_margin_right"
        android:layout_marginBottom = "80dp"
        android:id="@+id/fab_basement"
        android:src="@drawable/letterb"
        android:elevation="6dp"
        app:pressedTranslationZ="12dp"
        app:backgroundTint="@color/white"
        android:visibility="invisible"
        >
    </android.support.design.widget.FloatingActionButton>
    <!--plus button-->
    <android.support.design.widget.FloatingActionButton
        android:layout_width = "wrap_content"
        android:layout_height = "wrap_content"
        android:layout_gravity = "bottom|end"
        android:layout_marginRight = "@dimen/fab_margin_right"
        android:layout_marginBottom = "16dp"
        android:id="@+id/plus"
        android:src="@drawable/plus_icon"
        android:elevation="6dp"
        app:pressedTranslationZ="12dp"
        app:backgroundTint="@color/csusb_blue"
        ></android.support.design.widget.FloatingActionButton>


</android.support.design.widget.CoordinatorLayout>


